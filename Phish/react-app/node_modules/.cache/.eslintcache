 singleProductImage = Object?.values(productImages || {})[0]?.url\n    const productOwner = product?.productOwner\n\n    console.log(cart)\n\n    useEffect(() => {\n        dispatch(getSingleProductThunk(productId))\n    }, [productId, dispatch])\n\n\n    const handleDelete = async (e) => {\n        e.preventDefault()\n\n        await dispatch(deleteProductThunk(productId))\n\n        history.push('/')\n    }\n\n    return (\n        <main className='main-product-details'>\n            <div className='product-detail-container'>\n                <div className='product-detail-left-container'>\n                    <div className='product-detail-image'>\n                        <img id='display-img' src={singleProductImage} alt=''></img>\n                    </div>\n                    <div className='edit-delete-btns-container'>\n                        {user?.id  === productOwner?.id &&\n                            <div className='edit-delete-btns'>\n                                <div className='edit-product-btn'>\n                                <Link to={`/products/${productId}/edit`}>Edit</Link>\n                                </div>\n                                <div className='delete-product-btn'>\n                                <button id='delete-product' onClick={handleDelete}>Delete</button>\n                                </div>\n                            </div>\n                        }\n                    </div>\n                    {/* <div className='customer-reviews-container'>\n                        Reviews\n                    </div> */}\n                    <div className='product-description-container'>\n                        <h2>\n\n                        Description\n                        </h2>\n                        <br></br>\n                        <div className='product-description-body'>\n                        {product.description}\n                        </div>\n                    </div>\n                </div>\n                <div className='product-detail-right-container'>\n                    <div className='product-detail-right'>\n                        <p className='product-right-name'>\n                            {product?.name}\n                        </p>\n                        {/* <p>\n                            {product?.description}\n                        </p> */}\n                        <p className='product-price-right'>\n                            ${product?.price}\n                        </p>\n                    </div>\n                    <button className='add-to-cart-btn'\n                    onClick={async (e) => {\n                        e.preventDefault()\n                        if(user) {\n                            await dispatch(postCartItemThunk(productId))\n                            history.push('/cart')\n                        } else {\n                            history.push('/signin')\n                        }\n                    }}\n                    >\n                    Add to cart\n                    </button>\n                </div>\n                {/* {product.productImage} */}\n            </div>\n        </main>\n    )\n}\n\n\n\nexport default ProductDetails\n","/home/cahzzm/aug-CoHort/projects/Capstone/Phish/Phish/Phish/react-app/src/components/CreateProduct/index.js",["120"],"import React, { useEffect, useState } from 'react'\nimport { useDispatch } from 'react-redux'\nimport { Link } from 'react-router-dom/cjs/react-router-dom'\nimport { useHistory } from 'react-router-dom/cjs/react-router-dom.min'\nimport { createProductThunk } from '../../store/all_products'\nimport './CreateProduct.css'\n\n\nconst CreateProduct = () => {\n    const dispatch = useDispatch()\n    const history = useHistory()\n\n    const [name, setName] = useState('')\n    const [description, setDescription] = useState('')\n    const [price, setPrice] = useState(0)\n    const [imgUrl, setImgUrl] = useState('')\n    const [errors, setErrors] = useState([])\n\n\n    const handleSubmit = async (e) => {\n        e.preventDefault()\n\n        const newProduct = await dispatch (\n            createProductThunk({\n                name,\n                description,\n                price,\n                preview_img_url: imgUrl\n            })\n        )\n\n        // console.log('this is the new product',newProduct)\n        if (newProduct) {\n            newProduct.errors ? setErrors(newProduct.errors) : history.push(`/products/${newProduct.id}`)\n        }\n    }\n\n\n    // useEffect(() => {\n    //     const errs = []\n    //     if(name.length < 3 || name.length >= 25) errs.push(\"Name must be between 3 and 25 characters\")\n    //     if(description.length < 10 || name.length < 200) errs.push(\"Description must be between 10 and 200 characters\")\n    //     if(price <= 0 || price > 9999) errs.push('Make the people spend some money but not too much!')\n    //     if(imgUrl.length === 0) errs.push('Please provide an image url for your product')\n    //     setErrors(errs)\n    // }, [name, description, price, imgUrl])\n\n\n    return (\n        <main className='product-form-main'>\n            <form  className='product-add-form' onSubmit={handleSubmit}>\n                <div className='product-form-details-container'>\n                    <div className='product-form-header'>\n                        Listing Details\n                    </div>\n                    <div className='errors-create-form'>\n                        {errors.map(error => (\n                            <span key={error}>\n                                {error}\n                            </span>\n                        ))}\n                    </div>\n                    <div className='product-form-name-container'>\n                        <label>\n                            Name\n                        </label>\n                            <input\n                                type='text'\n                                placeholder='Be clear, concise, and descriptive of the product being sold'\n                                value={name}\n                                onChange={e => setName(e.target.value)}\n                            />\n                    </div>\n                    <div className='product-form-description-container'>\n                        <label>\n                            Description\n                        </label>\n                            <textarea\n                                type='textarea'\n                                placeholder='Add details about your product to increase sales'\n                                value={description}\n                                onChange={e => setDescription(e.target.value)}\n                            />\n                    </div>\n                    <div className='product-form-name-container'>\n                        <label>\n                            Image\n                        </label>\n                            <input\n                                type='url'\n                                placeholder='Image URL'\n                                value={imgUrl}\n                                onChange={e => setImgUrl(e.target.value)}\n                            />\n                    </div>\n                    <div className='product-form-price-container'>\n                        <label>\n                            Price\n                        </label>\n                            <input\n                                type='number'\n                                placeholder='Price'\n                                value={price}\n                                onChange={e => setPrice(e.target.value)}\n                            />\n                    </div>\n                </div>\n                <div className='product-form-btn-container'>\n                    <button className='create-product-btn' type='submit' onSubmit={handleSubmit}>\n                        Add to Listings\n                    </button>\n                </div>\n            </form>\n                    <div className='product-form-cancel'>\n                        <Link exact to='/'>\n                            Cancel\n                        </Link>\n                    </div>\n        </main>\n    )\n}\n\n\nexport default CreateProduct\n","/home/cahzzm/aug-CoHort/projects/Capstone/Phish/Phish/Phish/react-app/src/components/auth/Demo.js",[],"/home/cahzzm/aug-CoHort/projects/Capstone/Phish/Phish/Phish/react-app/src/store/all_products.js",[],"/home/cahzzm/aug-CoHort/projects/Capstone/Phish/Phish/Phish/react-app/src/store/cart.js",[],"/home/cahzzm/aug-CoHort/projects/Capstone/Phish/Phish/Phish/react-app/src/store/single_product.js",[],"/home/cahzzm/aug-CoHort/projects/Capstone/Phish/Phish/Phish/react-app/src/store/cart_item.js",[],"/home/cahzzm/aug-CoHort/projects/Capstone/Phish/Phish/Phish/react-app/src/components/EditProduct/index.js",["121","122"],"import React, { useState } from 'react'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { Link } from 'react-router-dom/cjs/react-router-dom'\nimport { useHistory } from 'react-router-dom/cjs/react-router-dom.min'\nimport { putSingleProductThunk } from '../../store/single_product'\nimport './EditProduct.css'\n\n\nconst EditProduct = () => {\n    const dispatch = useDispatch()\n    const history = useHistory()\n\n    const productDetails = useSelector(state => state.product.singleProduct)\n    const productImages = productDetails.productImages\n    const singleProductImage = Object?.values(productImages || {})[0]?.url\n    const [name, setName] = useState(productDetails.name)\n    const [description, setDescription] = useState(productDetails.description)\n    const [price, setPrice] = useState(productDetails.price)\n    const [imgUrl, setImgUrl] = useState(singleProductImage)\n    const [errors, setErrors] = useState([])\n\n    console.log(productDetails)\n\n\n    const handleSubmit = async (e) => {\n        e.preventDefault()\n\n        const newProduct = {\n                name,\n                description,\n                price,\n\n            }\n\n        let editedProduct = await dispatch(putSingleProductThunk(newProduct, productDetails?.id))\n\n        // console.log('this is the new product',newProduct)\n        if (editedProduct) {\n            editedProduct.errors ? setErrors(editedProduct.errors) : history.push(`/products/${editedProduct.id}`)\n        }\n    }\n\n\n    return (\n        <main className='product-form-main'>\n            <form  className='product-add-form' onSubmit={handleSubmit}>\n                <div className='product-form-details-container'>\n                    <div className='product-form-header'>\n                        Listing Details\n                    </div>\n                    <div className='product-form-name-container'>\n                    <div className='errors-create-form'>\n                        {errors.map(error => (\n                            <span key={error}>\n                                {error}\n                            </span>\n                        ))}\n                    </div>\n                        <label>\n                            Name\n                        </label>\n                            <input\n                                type='text'\n                                placeholder={productDetails.name}\n                                value={name}\n                                onChange={e => setName(e.target.value)}\n                            />\n                    </div>\n                    <div className='product-form-description-container'>\n                        <label>\n                            Description\n                        </label>\n                            <textarea\n                                type='textarea'\n                                placeholder={productDetails.name}\n                                value={description}\n   